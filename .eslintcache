-grey mr-3' onClick={() => setCurrentFormGroup(currentFormGroup-1)}>\r\n                                    <FontAwesomeIcon icon={faArrowLeft}></FontAwesomeIcon>&nbsp;&nbsp;Back\r\n                                </Button>\r\n                                <Button className='btn btn--primary' disabled={email===''} onClick={() => setCurrentFormGroup(currentFormGroup+1)}>\r\n                                    Ok&nbsp;&nbsp;<FontAwesomeIcon icon={faArrowRight}></FontAwesomeIcon>\r\n                                </Button>\r\n                            </Form.Group>\r\n                        </Collapse>\r\n                        <Collapse in={currentFormGroup===2} unmountOnExit >\r\n                            <Form.Group controlId=\"formBasicTimezone\">\r\n                                <Form.Label>Time Zone *</Form.Label>\r\n                                <Form.Control \r\n                                    type=\"text\" \r\n                                    className=\"mb-3\" \r\n                                    placeholder=\"Type your answer\"\r\n                                    as=\"select\" \r\n                                    value={timeZone}\r\n                                    onChange={e => setTimeZone(e.target.value)} \r\n                                    required\r\n                                >\r\n                                    <option></option>\r\n                                    {Timezones.map( tz => (\r\n                                        <option key={tz.value} value={tz.value}>{tz.label}</option>\r\n                                    ))}\r\n                                </Form.Control>\r\n                                <Button className='btn btn--grey mr-3' onClick={() => setCurrentFormGroup(currentFormGroup-1)}>\r\n                                    <FontAwesomeIcon icon={faArrowLeft}></FontAwesomeIcon>&nbsp;&nbsp;Back\r\n                                </Button>\r\n                                <Button 